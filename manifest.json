{
    "manifest_version": 3,
    "name": "9GAG Tag Blocker",
    "version": "1.0",
    "description": "A plugin to block 9GAG articles by tag.",
    "icons": {
        "48": "icons/icon-48.png",
        "96": "icons/icon-96.png",
        "128": "icons/icon-128.png"
    },
    "permissions": [
      "activeTab"
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "{70550fa5-8abb-4dbb-a6cd-6c68bf4a865f}"
      }
    },    
    "content_scripts": [
      {
        "matches": ["*://9gag.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  